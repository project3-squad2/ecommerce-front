{{#each monsters}}

 <div id="monster" data-id={{_id}} class="col-md-4 img" data-toggle="modal" data-target="#item" height="260px">
   <img  class="monster-images" src="{{url}}"  height="400" width="100%" />
   <h5 class="monster-name">{{name}}</h5>
   <h5 class="monster-price">${{price}}</h5>
 </div>

{{/each}}
