

 {{#each this}}
      {{#each monsters}}

      <div class="content">
        <img src="{{this.url}}" data-image-id="{{_id}}"alt="" />
        <h5 class="monster-name">{{name}}</h5>
        <h5 class="monster-price">${{price}}</h5>
        <h5 class="monster-name">{{description}}</h5>

        <button type="button" class="btn btn-warning delete-monster" data-monster-id="{{_id}}" data-toggle="modal" data-target="#delete-monster-modal">Delete</button>
        </div>
        <button type="button" class="btn btn-warning update-monster" data-monster-id="{{_id}}" data-toggle="modal" data-target="#delete-monster-modal">Update</button>
        </div>


   {{/each}}
 {{/each}}
